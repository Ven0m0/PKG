        "uBlock0@raymondhill.net": {
          "toAdd": {
            "selectedFilterLists": [
              "adguard-annoyance",
              "adguard-generic",
              "adguard-spyware-url",
              "easylist",
              "easyprivacy",
              "ublock-abuse",
              "ublock-annoyances",
              "ublock-badware",
              "ublock-filters",
              "ublock-privacy",
              "ublock-unbreak",
              "user-filters"
            ]
          }
        }
      }
    },
    "UserMessaging": {
      "FeatureRecommendations": false,
      "ExtensionRecommendations": false,
      "WhatsNew": false,
      "UrlbarInterventions": false,
      "SkipOnboarding": true,
      "FirefoxLabs": false,
      "MoreFromMozilla": false,
      "Locked": true
